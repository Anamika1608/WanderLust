{
    "version" : 2,
    "builds" : [
        {
            "src" : "./app.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/app.js"
        }
    ],
    "functions": {
        "views/listings/users/**/*.js": {
        "maxDuration": 30
    }
    }
}
